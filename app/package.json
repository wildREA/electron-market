  "description": "",
  "build": {
    "appId": "com.electron.market",
    "productName": "Electron Market",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.utilities",
      "icon": "app/images/icons/app_icon.png"
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "app/images/icons/app_icon.png"
    },
    "linux": {
      "target": [
        "AppImage",
        "zip"
      ],
      "icon": "app/images/icons/app_icon.png"
    },
    "directories": {
      "output": "dist"
    },
    "artifactName": "${productName}-${version}-${os}.${ext}",
    "publish": null
  }
}